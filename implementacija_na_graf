#include <bits/stdc++.h>

using namespace std;

int main()
{
    //implementacija so lista na sosedstvo
    int n,m,a,b;
    cin>>n>>m;
    vector<int> G[n+1];
    for(int i=0 ; i<m ; i++)
    {
        cin>>a>>b;
        G[a].push_back(b);
        //G[b].push_back(a);    //za dvonasocen
    }

    //implementacija so matrica na sosedstvo;
    bool mat[n+1][n+1];
    memset(mat,0,sizeof(0));
    for(int i=0 ; i<m ; i++)
    {
        cin>>a>>b;
        mat[a][b]=1;
        //mat[b-1][a-1]=1;  //za dvonasocen
    }

    /*
    for (int i=0 ; i<n ; i++)
    {
        cout<<"Sosedi na temeto "<<i<<" se:"<<endl;
        for(auto next : G[i])
            cout<<next<<" ";
    }
    cout<<endl;
    for (int i=0 ; i<n ; i++)
    {
        cout<<"Sosedi na temeto "<<i<<" se:"<<endl;
        for (int j=0 ; j<n ; j++)
        {
            if(mat[i][j])
                cout<<j<<" ";
        }
        cout << endl;
    }
    */
    return 0;
}
